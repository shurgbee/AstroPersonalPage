---
import Card from "../Card/index.astro";

interface Props {
    link?: string;
    name: string;
    github?: string;
    blurb?: string;
}

const {link, name, blurb, github} = Astro.props
---

<Card colSpan="md:col-span-1" rowSpan="md:row-span-8">
<div class="flex-wrap ">
    <h1 class="font-bold">{name}</h1>  
</div>
</Card>